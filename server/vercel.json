{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/users/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/v1/company/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/v1/jobs/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/v1/application/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}